{
  "title": "Sino's mac open apps",
  "maintainers": ["sino"],
  "homepage": "https://github.com/SinoThomas/macOS-karabiner-shortcuts",
  "import_url": "karabiner://karabiner/assets/complex_modifications/import?url=https://raw.githubusercontent.com/SinoThomas/macOS-karabiner-shortcuts/master/sinos_mac_open_apps.json",
  "rules": [
    {
      "description": "Fn + F              Open Finder",
      "manipulators": [
        {
          "type": "basic",
          "from": { "key_code": "f", "modifiers": { "mandatory": ["fn"] } },
          "to": [{ "shell_command": "open -a 'finder'" }]
        }
      ]
    },
    {
      "description": "Fn + T              Open Terminal",
      "manipulators": [
        {
          "type": "basic",
          "from": { "key_code": "t", "modifiers": { "mandatory": ["fn"] } },
          "to": [{ "shell_command": "open -a 'terminal'" }]
        }
      ]
    },
    {
      "description": "Fn + C              Open Google Chrome",
      "manipulators": [
        {
          "type": "basic",
          "from": { "key_code": "c", "modifiers": { "mandatory": ["fn"] } },
          "to": [{ "shell_command": "open -a 'Google Chrome'" }]
        }
      ]
    },
    {
      "description": "Fn + X              Open Firefox Developer Edition",
      "manipulators": [
        {
          "type": "basic",
          "from": { "key_code": "x", "modifiers": { "mandatory": ["fn"] } },
          "to": [{ "shell_command": "open -a 'Firefox Developer Edition'" }]
        }
      ]
    },
    {
      "description": "Fn + Z              Open Figma",
      "manipulators": [
        {
          "type": "basic",
          "from": { "key_code": "z", "modifiers": { "mandatory": ["fn"] } },
          "to": [{ "shell_command": "open -a 'Figma'" }]
        }
      ]
    },
    {
      "description": "Fn + V              Open Intellij Idea",
      "manipulators": [
        {
          "type": "basic",
          "from": { "key_code": "v", "modifiers": { "mandatory": ["fn"] } },
          "to": [{ "shell_command": "open -a 'Intellij Idea'" }]
        }
      ]
    },
    {
      "description": "Fn + B              Open Visual Studio Code",
      "manipulators": [
        {
          "type": "basic",
          "from": { "key_code": "b", "modifiers": { "mandatory": ["fn"] } },
          "to": [{ "shell_command": "open -a 'Visual Studio Code'" }]
        }
      ]
    },

    {
      "description": "Fn + Shift + C              Open Google Chrome",
      "manipulators": [
        {
          "type": "basic",
          "from": { "modifiers": { "mandatory": ["fn", "left_shift"] }, "key_code": "c" },
          "to": [{ "shell_command": "open -a 'Google Chrome'" }]
        }
      ]
    },
    {
      "description": "Fn + Shift + X              Open Firefox Developer Edition",
      "manipulators": [
        {
          "type": "basic",
          "from": { "modifiers": { "mandatory": ["fn", "left_shift"] }, "key_code": "x" },
          "to": [{ "shell_command": "open -a 'Firefox Developer Edition'" }]
        }
      ]
    },
    {
      "description": "Fn + Shift + S              Open Safari",
      "manipulators": [
        {
          "type": "basic",
          "from": { "modifiers": { "mandatory": ["fn", "left_shift"] }, "key_code": "s" },
          "to": [{ "shell_command": "open -a 'Safari'" }]
        }
      ]
    },
    {
      "description": "Fn + Shift + O              Open Opera",
      "manipulators": [
        {
          "type": "basic",
          "from": { "modifiers": { "mandatory": ["fn", "left_shift"] }, "key_code": "o" },
          "to": [{ "shell_command": "open -a 'Opera'" }]
        }
      ]
    },
    {
      "description": "Fn + Shift + B             Open Brave Browser",
      "manipulators": [
        {
          "type": "basic",
          "from": { "modifiers": { "mandatory": ["fn", "left_shift"] }, "key_code": "b" },
          "to": [{ "shell_command": "open -a 'Brave Browser'" }]
        }
      ]
    },
    {
      "description": "Fn + Shift + E              Open Microsoft Edge",
      "manipulators": [
        {
          "type": "basic",
          "from": { "modifiers": { "mandatory": ["fn", "left_shift"] }, "key_code": "e" },
          "to": [{ "shell_command": "open -a 'Microsoft Edge'" }]
        }
      ]
    },

    {
      "description": "Fn + Shift + D              Open Intellij Idea",
      "manipulators": [
        {
          "type": "basic",
          "from": { "modifiers": { "mandatory": ["fn", "left_shift"] }, "key_code": "d" },
          "to": [{ "shell_command": "open -a 'Intellij Idea'" }]
        }
      ]
    },
    {
      "description": "Fn + Shift + I              Open Intellij Idea",
      "manipulators": [
        {
          "type": "basic",
          "from": { "modifiers": { "mandatory": ["fn", "left_shift"] }, "key_code": "i" },
          "to": [{ "shell_command": "open -a 'Intellij Idea'" }]
        }
      ]
    },
    {
      "description": "Fn + Shift + V              Open Visual Studio Code",
      "manipulators": [
        {
          "type": "basic",
          "from": { "modifiers": { "mandatory": ["fn", "left_shift"] }, "key_code": "v" },
          "to": [{ "shell_command": "open -a 'Visual Studio Code'" }]
        }
      ]
    },
    {
      "description": "Fn + Shift + F              Open Figma",
      "manipulators": [
        {
          "type": "basic",
          "from": { "modifiers": { "mandatory": ["fn", "left_shift"] }, "key_code": "f" },
          "to": [{ "shell_command": "open -a 'Figma'" }]
        }
      ]
    },

    {
      "description": "Fn + Shift + Option + Z      Open New Safari App",
      "manipulators": [
        {
          "type": "basic",
          "from": { "modifiers": { "mandatory": ["fn", "left_shift", "left_option"] }, "key_code": "z" },
          "to": [{ "shell_command": "open -a 'Safari' --new" }]
        }
      ]
    }
  ]
}
